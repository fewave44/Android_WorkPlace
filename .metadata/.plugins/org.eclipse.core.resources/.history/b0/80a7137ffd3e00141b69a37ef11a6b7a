package com.example;

import myhttp.*;
public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyHttp http = new MyHttp();
		String respondStr = "";
		String url = "http://translate.google.cn/translate_a/single?client=t&sl=en&tl=zh-CN&hl=zh-CN&dt=bd&dt=ex&dt=ld&dt=md&dt=qc&dt=rw&dt=rm&dt=ss&dt=t&dt=at&dt=sw&ie=UTF-8&oe=UTF-8&prev=btn&srcrom=1&ssel=3&tsel=0&q=hello";
		try
		{
			respondStr = http.readHttpRespondFromGet(url);
		}
		catch (Exception e)
		{
			System.out.println(e);
		}
		System.out.println(respondStr);
	}

}
